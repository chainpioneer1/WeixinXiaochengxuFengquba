/**app.wxss**/

.container {
    width: 100vw;
    height: 100vh;
    /*
  display: flex;
  */
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0 0;
    box-sizing: border-box;
    background-color: #F4F4F4;
    font-family: PingFangSC-Regular;
}

page {
    background-color: #F4F4F4;
}

button::after {
    outline: none;
    border: none;
}

button {
    box-sizing: unset;
    overflow: unset;
    padding: 0;
}