}

.modal-confirm1,
.modal-cancel1 {
    display: inline-block;
    width: 85%;
    height: 78rpx;
    top: 0;
    line-height: 78rpx;
    background: #e6b53c;
    border-radius: 10rpx;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    letter-spacing: 0.43rpx;
    -webkit-border-radius: 10rpx;
    -moz-border-radius: 10rpx;
    -ms-border-radius: 10rpx;
    -o-border-radius: 10rpx;
}